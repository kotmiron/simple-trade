<html>
  <head>
    <meta charset="utf-8">
    <title>test</title>
  </head>
  <body>
    <a href="users/roles">Роли</a>
    <br><br>
	Логины:
    <a href="logins/add">Добавить</a>
    <br>
    <table border="1">
      <tr>
        <td>
          ID
        </td>
        <td>
          Логин
        </td>
        <td>
          Действия
        </td>
      </tr>
      {% for login in logins %}
        <tr>
          <td>
            {{ login.getId() }}
          </td>
          <td>
            {{ login.getLogin() }}
          </td>
          <td>
            <a href="logins/remove/{{ login.getId() }}">удалить</a>
            <br>
            <a href="logins/change/{{ login.getId() }}">изменить</a>
            <br>
            <a href="users/show/{{ login.getId() }}">Пользователи</a>
          </td>
        </tr>
      {% endfor %}
    </table>
  </body>
</html>
